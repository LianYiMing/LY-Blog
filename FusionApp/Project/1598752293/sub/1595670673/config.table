function getCardlist(t)\
  return loadlayout({\
    LinearLayout,--线性布局\
    orientation=\"vertical\",--布局方向\
    layout_width=\"120dp\",--布局宽度\
    layout_height=\"45dp\",--布局高度\
    onClick=function(v)\
      print(v.getChildAt(0).getChildAt(0).Text)\
    end,\
    {\
      CardView,--卡片框控件\
      layout_width=\"107dp\",--布局宽度\
      layout_height=\"37dp\",--布局高度\
      elevation=\"5dp\";--阴影层次\
      layout_margin=\"2dp\",\
      layout_marginLeft='3%w';--布局左距\
      --cardBackgroundColor=\"#FF\",--卡片背景色\
      radius=\"19dp\",--圆角半径\
\
      {\
        TextView,--文本框控件\
        text=t,--文本内容\
        textSize=\"14sp\",--文本大小\
        textColor=\"#ff000000\",--文本颜色\
        layout_gravity='center';--重力居中\
      }, \
    }--卡片框控件 结束\
  })\
end\
local function add(id,tab,f)\
  for k,v in pairs(tab) do\
    id.addView(f(v))\
  end\
end\
