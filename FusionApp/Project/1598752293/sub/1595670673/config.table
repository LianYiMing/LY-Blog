function getImageview(tab)\
  return loadlayout( {\
    LinearLayout,--线性布局\
    orientation=\"vertical\",--布局方向\
    layout_width=\"wrap\",--布局宽度\
    layout_height=\"wrap\",--布局高度\
    {\
      CardView,--卡片框控件\
      layout_width=\"100dp\",--布局宽度\
      layout_height=\"130dp\",--布局高度\
      layout_margin=\"10dp\",--布局边距\
      \
      radius=\"10dp\",--圆角半径\
      {\
        ImageView,--图片框控件\
        layout_width=\"fill\",--布局宽度\
        layout_height=\"fill\",--布局高度\
        src=tab[1],\
        scaleType=\"fitXY\",--图片显示类型\
      },\
      {\
        TextView,--文本框控件\
        text=tab[2],--文本内容\
        layout_width=\"fill\",\
        layout_height=\"20dp\",\
        textSize=\"15sp\",--文本大小\
        layout_gravity='bottom';--重力居中｜置底\
        gravity='center';--重力居中｜置底\
        textColor=\"#ffffff\",--文本颜色\
        background='#00000000';--布局背景\
      },\
    },--卡片框控件 结束\
  }--线性布局 结束\
  )\
end\
