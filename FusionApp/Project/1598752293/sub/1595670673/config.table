function addCardView(v,f)\
  _root.addView(loadlayout(\
  {\
    LinearLayout,--线性布局\
    orientation=\"vertical\",--布局方向\
    layout_width=\"fill\",--布局宽度\
    layout_height=\"wrap\",--布局高度   \
    {\
      CardView,--卡片框控件\
      layout_width=\"fill\",--布局宽度\
      layout_height=f or \"180dp\" ,--布局高度\
      layout_marginTop='-2dp';--布局顶距\
      layout_margin=\"10dp\",--布局边距\
      cardBackgroundColor=\"#ffffff\",--卡片背景色\
      radius=\"10dp\",--圆角半径\
      elevation=\"8dp\";--阴影层次\
      v,\
    }--卡片框控件 结束\
  }\
  ))\
end\
