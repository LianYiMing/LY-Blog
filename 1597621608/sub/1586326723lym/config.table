function 获取一文()\
  Http.get(\"https://meiriyiwen.com/random/iphone\",nil,nil,nil,function(code,content)\
    if code==200 then\
      title=content:match([[<head>\
        <title>(.-) @每日一文官网 </title>]])\
      标题=title:match(\"(.-)%-\")\
      作者=title:match(\"-(.+)\")\
      内容=content:match([[<div%sclass=\"articleContent\">(.-)</div>]]):gsub(\"<p>\",\"\"):gsub(\"</p>\",\"\\n\")\
\
      标题text.text=标题\
      作者text.text=作者\
      内容text.text=内容\
\
    else\
      标题text.text=\"获取失败\"\
      作者text.text=\"获取失败\"\
      内容text.text=code\
    end\
  end)\
end\
